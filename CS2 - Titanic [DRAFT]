def p_class(input):
    with open('class.csv', 'w') as output

        # Skip header row
        next(input)

        first_class = 0
        second_class = 0
        third_class = 0
       
        for line in input:
            fields = line.strip().split(',')
       
            class = fields[5]
           
            if class == "1":
                first_class += 1
            elif class == "2":
                second_class += 1
            elif class == "3":
                third_class += 1
           
        output.write("Male" + "," + "Female")
        output.write("\n")
        output.write(str(male_count) + "," + str(female_count))


def main():
    input_file = 'titanic.csv'
    with open(input_file, 'r') as input:
        p_gender(input)

if __name__ == '__main__':
    main()


