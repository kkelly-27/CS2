
def p_gender(input):
    with open('genders.csv', 'w') as output:
       
        # Skip header row
        next(input)
       
        male_count = 0
        female_count = 0
       
        for line in input:
            fields = line.strip().split(',')
       
            sex = fields[5]
           
            if sex == "male":
                male_count += 1
            elif sex == "female":
                female_count += 1
           
        output.write("Male" + "," + "Female")
        output.write("\n")
        output.write(str(male_count) + "," + str(female_count))

